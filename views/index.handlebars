<!-- Banner -->
<section id="banner">
	<div class="content">
		<h1>Dating App</h1>
		<p>Save your date
			<br />by inviting your friends to join the conversation.</p>
		<ul class="actions">
			<li>
				<a href="#one" class="button scrolly">Get Started</a>
			</li>
		</ul>
	</div>
</section>

<!-- One -->
<section id="one" class="wrapper">
	<div class="inner flex flex-3">
		<div class="flex-item left">
			<div id="test"></div>
			<img id="testimage" src="" style="float:left" />


		</div>
		<div class="flex-item right">
			<div>
				<h3>Feature 3</h3>
				<button type="button" id="testbtn">Add to DB Test</button>
				<input type="text" id="searchInput">
				<button type="button" id="searchBtn" method="PUT" action="/api/examples/:id">Add to FriendsList</button>

			</div>
			<div>
				<div id="friendsList"></div>
				<h3>Friend List:</h3>
				<ul id="example-list" class="list-group">
					{{#each examples}}
					<li data-id="{{this.id}}" class="list-group-item">
						<a href="example/{{this.id}}">{{this.facebookID}}</a>
						<button class="btn btn-danger float-right delete">ï½˜</button>
					</li>
					{{/each}}
				</ul>



				<h3>Match List</h3>
				<div id="matchList"></div>

				


			</div>
		</div>
	</div>
</section>

<div id="talkjs-container" style="width: 90%; margin: 30px; height: 500px"><i>Loading chat...</i></div>